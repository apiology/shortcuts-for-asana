{
  "name": "Shortcuts for Asana",
  "description": "Chrome extension which adds missing keyboard shortcuts/behavior to Asana",
  "version": "0.1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["dist/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://app.asana.com/*"],
      "js": ["config.js", "dist/content-script.js"]
    }
  ],
  "manifest_version": 2
}
