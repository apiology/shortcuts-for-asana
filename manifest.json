{
  "name": "Shortcuts for Asana",
  "description": "Chrome extension which adds missing keyboard shortcuts/behavior to Asana",
  "version": "0.1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["config.js", "dist/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}
